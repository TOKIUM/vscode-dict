{
  "name": "vscode-dict",
  "displayName": "vscode-dict",
  "description": "vscode plugin for dict.yml",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/TOKIUM/vscode-dict"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "ruby",
        "extensions": [
          ".rb"
        ]
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscode-dict",
          "title": "vscode-dict",
          "icon": "resources/dict.svg"
        }
      ]
    },
    "views": {
      "vscode-dict": [
        {
          "id": "vscode-dict-explorer",
          "name": "vscode-dict-explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-dict.reload",
        "title": "vscode-dict: Reload vscode-dict",
        "icon": "$(extensions-refresh)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "vscode-dict.reload",
          "when": "view == vscode-dict-explorer",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "prebuild": "npm run test",
    "build": "vsce package -o vscode-dict.vsix",
    "lint": "eslint src --ext ts",
    "test": "mocha ./out/core/**/*.test.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "@vscode/vsce": "^2.19.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@tokiumjp/dict": "^0.1.25",
    "js-yaml": "^4.1.0"
  }
}
